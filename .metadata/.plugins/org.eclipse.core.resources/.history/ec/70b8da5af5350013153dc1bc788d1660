package main.formes.util;

import main.formes.Forme;

public class ReplacerForme extends AnalyseurTrame {
	
	public ReplacerForme(String trame) {
		super(trame);
		
	}

	private int numero=0;
	private int x1, y1, x2, y2, centreX, centreY, rayonH, rayonV, rayon, numeroSequence;
	
	public void setX1(int x1){
		this.x1 = x1;
	}
	
	public void setY1(int y1){
		this.y1=y1;
	}

	
	public int replacerForme(){
		x1 = getX1() * numero;
		y1 = getY1() * numero;
		
		x2 = getX2() * numero;
		y2 = getY2() * numero;
		
		
			
		return numero;
			
	}
			

	

}
